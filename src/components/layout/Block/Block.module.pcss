.container {
    width: 100%;
    padding: 0 130px;
    padding-bottom: var(--sp-block-v);
}


@media (max-width: 768px) {
    .container {
        padding: 0 var(--sp-lg);
        padding-bottom: var(--sp-xxl);
    }
}

@media (max-width: 480px) {
    .container {
        padding: 0 var(--sp-md);
        padding-bottom: var(--sp-xxl);
    }
}
