.container {
  width: 100%;
  padding: 0 130px;
  padding-bottom: var(--sp-block-v);
  background-color: var(--color-base-white);
  z-index: 1;
}

@media (width <= 768px) {
  .container {
    padding: 0 var(--sp-lg);
    padding-bottom: var(--sp-xxl);
  }
}

@media (width <= 480px) {
  .container {
    padding: 0 var(--sp-md);
    padding-bottom: var(--sp-xxl);
  }
}
