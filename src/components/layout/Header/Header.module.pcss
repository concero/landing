.container {
    width: 100%;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    padding: var(--sp-md) 0;

    .buttonText {
        color: var(--color-primary-500);
    }

    .buttonIcon {
        color: var(--color-primary-500);
        transform: rotate(90deg);
    }
}

.anime {
    rotate: 0deg;
    transition: all 0.3s ease-in-out;
}

.mainButton:hover .anime {
    rotate: 45deg;
}
