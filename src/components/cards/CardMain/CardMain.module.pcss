.innerContainer {
    width: 100%;
    align-items: center;
    justify-content: center;
}

.imgContainer {
    padding-top: var(--sp-xl);
    margin-bottom: calc(var(--sp-xxl) * -1);
    flex: 1;
    justify-content: flex-end;

    img {
        width: 100%;
        height: 100%;
        object-fit: cover;
    }
}
