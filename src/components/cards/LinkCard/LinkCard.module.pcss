.container {
    align-items: center;
    justify-content: center;
    width: 100px;
    height: 100px;
}

.iconContainer {
    padding: var(--sp-lg);
    align-items: center;
    justify-content: center;
    width: 100%;
    height: 100%;

    .icon {
        width: 100%;
        height: 100%;
        background-size: cover;
        background-position: center;
        background-repeat: no-repeat;
    }
}
