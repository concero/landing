.chainsContainer {
    width: 100%;
    flex-direction: row;
    flex-wrap: wrap;
    gap: var(--sp-sm);
}

.chainCardContainer {
    width: 110px;
    height: 110px;
    justify-content: center;
    align-items: center;
    background-color: var(--color-bg1);
    border-radius: 50%;

    img {
        width: 50%;
        height: 50%;
        border-radius: 50%;
    }
}
